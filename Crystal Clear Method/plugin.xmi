            <methodElementProperty xmi:id="_zj330Ps7EeiOY5TxRmxENA" name="me_edited" value="true"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Artifact" xmi:id="_70KTAPs7EeiOY5TxRmxENA" name="unit test" guid="_70KTAPs7EeiOY5TxRmxENA" presentationName="unit test">

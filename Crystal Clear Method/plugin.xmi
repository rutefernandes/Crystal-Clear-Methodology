          <contentElements xsi:type="org.eclipse.epf.uma:Task" xmi:id="_kG2H8PshEeiOY5TxRmxENA" name="Code Integration" guid="_kG2H8PshEeiOY5TxRmxENA" presentationName="Code Integration">
